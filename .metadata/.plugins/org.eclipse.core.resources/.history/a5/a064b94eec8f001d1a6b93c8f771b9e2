package threads;

public class Charjoinint extends Thread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try
		{
			Charjoinint t1 = new Charjoinint();
			Intjoin t2 = new Intjoin();
			t1.start();
			t1.join();
			t2.start();
			
		}
		catch(Exception e){
			
		}

	}

}
